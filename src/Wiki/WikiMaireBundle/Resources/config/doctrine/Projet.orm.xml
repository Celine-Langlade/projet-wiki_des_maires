<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Wiki\WikiMaireBundle\Entity\Projet" table="projet">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>

    <field name="tailleprojet" type="string" column="tailleprojet" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="bassinpopulation" type="string" column="bassinpopulation" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="difficulte" type="string" column="difficulte" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cout" type="float" column="cout" precision="10" scale="0" nullable="false"/>
    <field name="nomprojet" type="string" column="nomprojet" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="commune" type="string" column="commune" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="expert" type="string" column="expert" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>

  </entity>
</doctrine-mapping>
